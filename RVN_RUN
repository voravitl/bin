#!/bin/bash
LOGS="/opt/logs/RVN.log.`date +%d-%b-%Y`"
RVN trade \
zenbot sim binance.RVN-BTC  \
--strategy=multi \
--period_length=3m \
--min_periods=52 \
--markdown_buy_pct=0 \
--markup_sell_pct=0 \
--overbought_rsi_periods=25 \
--overbought_rsi=74 \
--poll_trades=5000 \
--order_poll_time=5000 \
--order_adjust_time=5000 \
--length=11 \
--fish_pct_change=0 \
--pos_length=2 \
--src=ohlc4 \
--momentum_size=7 \
--max_sell_loss_pct=3 \
--profit_stop_enable_pct=5 \
--markdown_buy_pct=0.33 \
--markup_sell_pct=0.32 \
--sell_pct=100 --buy_pct=100 --currency_capital=0.08  2>&1 | tee $LOGS
