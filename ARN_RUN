LOGS="/opt/logs/ARN.log.`date +%d-%b-%Y`"
ARN trade binance.ARN-BTC --strategy=neural --buy_pct=100 --sell_pct=100 --markup_sell_pct=0.8 --markdown_buy_pct=0.25 --period=1m --max_sell_loss_pct=0 --momentum=0.888 --activation_1_type=sigmoid  --threads=4 --poll_trades=3000  --order_poll_time=1500  --order_adjust_time=1500 --reset_profit 2>&1 | tee $LOGS 
