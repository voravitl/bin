#!/bin/bash
LOGS="/opt/logs/ETH.log.`date +%d-%b-%Y`"
ETH binance.ETH-BTC --strategy=trend_ema --markdown_buy_pct=0.18 --markup_sell_pct=0.17 --min_periods=57 --neutral_rate=0.0008 --oversold_rsi=8 --oversold_rsi_periods=21 --period=30s --profit_stop_enable_pct=4 --profit_stop_pct=0.3 --rsi_periods=21 --trend_ema=26 --max_sell_loss_pct=0.25 --max_slippage_pct=5  --buy_stop_pct=0 --sell_stop_pct=0 --poll_trades=3000 --order_poll_time=1500 --order_adjust_time=1500  --buy_pct=99 --sell_pct=99 --reset_profit  2>&1 | tee $LOGS
#ETH binance.ETH-BTC  --strategy=trend_ema --markdown_buy_pct=0.14 --markup_sell_pct=0.21 --min_periods=57 --neutral_rate=0.0008 --oversold_rsi=8 --oversold_rsi_periods=21 --period=30s --profit_stop_enable_pct=3 --profit_stop_pct=0.3 --rsi_periods=21 --trend_ema=26 --max_sell_loss_pct=0 --max_slippage_pct=5  --buy_stop_pct=0 --sell_stop_pct=0 --poll_trades=3000 --order_poll_time=1500 --order_adjust_time=1500  --buy_pct=99 --sell_pct=99 --reset_profit  2>&1 | tee $LOGS
#ETH trade gdax.ETH-BTC --strategy=trend_ema --markdown_buy_pct=0.25 --markup_sell_pct=0.25 --min_periods=52 --neutral_rate=auto --oversold_rsi=6 --oversold_rsi_periods=21 --period=40s --profit_stop_enable_pct=5 --profit_stop_pct=0.1 --rsi_periods=21 --trend_ema=26 --max_sell_loss_pct=0 --max_slippage_pct=5  --buy_stop_pct=7 --sell_stop_pct=0  --poll_trades=5000  --order_poll_time=5000  --order_adjust_time=15000   2>&1 | tee $LOGS

