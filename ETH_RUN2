#!/bin/bash
LOGS="/opt/logs/ETH2.log.`date +%d-%b-%Y`"
#ETH2 trade  gdax.ETH-BTC  --strategy=ta_ppo --markdown_buy_pct=0.24 --markup_sell_pct=0.17 --period=10s --ema_short_period=12 --ema_long_period=26 --signal_period=9 --overbought_rsi_periods=25  --ma_type=SMA --overbought_rsi=75 --max_sell_loss_pct=1.5  2>&1 | tee $LOGS

