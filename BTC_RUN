#BTC trade gdax.BTC-USD --strategy=trend_ema --markdown_buy_pct=0.45 --markup_sell_pct=0.2 --min_periods=52 --neutral_rate=auto --oversold_rsi=8 --oversold_rsi_periods=21 --period=20s --profit_stop_enable_pct=1 --profit_stop_pct=0.25 --rsi_periods=21 --trend_ema=26 --max_sell_loss_pct=0 --max_slippage_pct=5  --buy_stop_pct=7 --sell_stop_pct=0  --poll_trades=3000  --order_poll_time=1500  --order_adjust_time=10000 2>&1 | tee $LOGS
#BTC trade gdax.BTC-USD --strategy=trend_ema --markdown_buy_pct=0.4 --markup_sell_pct=0.25 --min_periods=52 --neutral_rate=0.02 --oversold_rsi=8 --oversold_rsi_periods=21 --period=35s --profit_stop_enable_pct=0 --profit_stop_pct=0.60 --rsi_periods=21 --trend_ema=26 --max_sell_loss_pct=0 --max_slippage_pct=5  --buy_stop_pct=7 --sell_stop_pct=0  --poll_trades=5000  --order_poll_time=5000  --order_adjust_time=10000 2>&1 | tee $LOGS

