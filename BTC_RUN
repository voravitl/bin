#!/bin/bash
LOGS="/opt/logs/BTC.log.`date +%d-%b-%Y`"
BTC trade gdax.BTC-USD --strategy=neural --buy_pct=100 --sell_pct=100 --markup_sell_pct=0.35 --markdown_buy_pct=0.1 --period=1m --max_sell_loss_pct=0 --momentum=0.888 --days=1 --currency_capital=341 --activation_1_type=sigmoid  --threads=4 --poll_trades=1500  --order_poll_time=1500  --order_adjust_time=1500 2>&1 | tee $LOGS 
