#!/bin/bash
LOGS="/opt/logs/BTC2.log.`date +%d-%b-%Y`"
BTC2 trade gdax.BTC-USD --strategy=trend_ema --markdown_buy_pct=0.45 --markup_sell_pct=0.25 --min_periods=57 --neutral_rate=0.008 --oversold_rsi=7 --oversold_rsi_periods=21 --period=10s --profit_stop_enable_pct=3 --profit_stop_pct=0.5 --rsi_periods=21 --trend_ema=12 --max_sell_loss_pct=0.3 --max_slippage_pct=5 --order_adjust_time=1500 --reset_profit --buy_stop_pct=7 --sell_stop_pct=0 --poll_trades=1500  --order_poll_time=1500  --order_adjust_time=1800 2>&1 | tee $LOGS
#BTC2 trade gdax.BTC-USD --strategy=trend_ema --markdown_buy_pct=0.4 --markup_sell_pct=0.25 --min_periods=52 --neutral_rate=0.02 --oversold_rsi=8 --oversold_rsi_periods=21 --period=35s --profit_stop_enable_pct=0 --profit_stop_pct=0.60 --rsi_periods=21 --trend_ema=26 --max_sell_loss_pct=0 --max_slippage_pct=5  --buy_stop_pct=7 --sell_stop_pct=0  --poll_trades=5000  --order_poll_time=5000  --order_adjust_time=10000 2>&1 | tee $LOGS
#BTC2 trade gdax.BTC-USD  --strategy=trend_ema --markdown_buy_pct=0.22 --markup_sell_pct=0.08 --min_periods=57 --neutral_rate=0.025 --oversold_rsi=8 --oversold_rsi_periods=21 --period=35s --profit_stop_enable_pct=1 --profit_stop_pct=0.1 --rsi_periods=21 --trend_ema=3 --max_sell_loss_pct=-0.26 --order_adjust_time=15000 --reset_profit --buy_stop_pct=1 --sell_stop_pct=0.3 --poll_trades=2000 --max_slippage_pct=0.1 2>&1 | tee $LOGS

